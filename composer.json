{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": "^7.3 || ^8.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "~4.10.0@stable",
        "silverstripe-themes/simp`le": "~3.2.0",
        "silverstripe/login-forms": "~4.6.0@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "expose": [
            "themes/my-theme/css",
            "themes/my-theme/javascript",
            "themes/my-theme/webfonts",
            "themes/my-theme/images",
            "themes/react/css",
            "themes/react/javascript",
            "themes/react/webfonts",
            "themes/react/images",
            "vendor/silverstripe/admin/client/dist",
            "vendor/silverstripe/admin/client/lang",
            "vendor/silverstripe/cms/client/dist",
            "vendor/silverstripe/cms/client/lang",
            "vendor/silverstripe/session-manager/client/dist",
            "vendor/silverstripe/versioned-admin/client/dist",
            "vendor/silverstripe/asset-admin/client/dist",
            "vendor/silverstripe/asset-admin/client/lang",
            "vendor/silverstripe/campaign-admin/client/dist",
            "vendor/silverstripe/campaign-admin/client/lang",
            "vendor/silverstripe/admin/thirdparty",
            "vendor/silverstripe/login-forms/client/dist"
        ],
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
